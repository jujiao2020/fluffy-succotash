{
  "name": "jscp/social-sdk",
  "type": "library",
  "version": "v1.0.0",
  "keywords": [
    "php",
    "sdk"
  ],
  "description": "social api sdk",
  "license": "Apache-2.0",
  "require": {
    "php": ">7.1"
  },
  "autoload": {
    "psr-4": {
      "Jscp\\SocialSdk\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Jscp\\SocialSdk\\Unit\\": "test/unit/",
      "Jscp\\SocialSdk\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
