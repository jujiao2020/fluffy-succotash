{
  "name": "jcsp/social-sdk",
  "type": "library",
  "version": "v1.2.3",
  "keywords": [
    "php",
    "sdk"
  ],
  "description": "social api sdk",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ext-curl": "*",
    "abraham/twitteroauth": "^1.0",
    "dirkgroenen/pinterest-api-php": "^0.2.12",
    "facebook/graph-sdk": "^5.7",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.5",
    "tumblr/tumblr": "^0.4.0",
    "vimeo/vimeo-api": "^3.0",
    "vkcom/vk-php-sdk": "^5.101"
  },
  "autoload": {
    "psr-4": {
      "Jcsp\\SocialSdk\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Jscp\\SocialSdk\\Unit\\": "test/unit/",
      "Jscp\\SocialSdk\\Testing\\": "test/testing/"
    }
  },
  "repositories": {
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
